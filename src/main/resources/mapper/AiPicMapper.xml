<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.aiserver.mapper.AiPicMapper">

    <select id="queryByMd5" resultType="com.example.aiserver.entity.AiPic">
        select *
        from ai_pic
        where md5 = #{md5}
    </select>
</mapper>
